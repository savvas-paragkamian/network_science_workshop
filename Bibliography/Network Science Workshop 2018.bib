doi = {10.1093/bioinformatics/btr311},
file = {:Users/savasp/Library/Mobile Documents/com{\~{}}apple{\~{}}Preview/Documents/Complexity and Networks/linkcomm$\backslash$: an R package for the generation, visualization, and analysis of link communities in networks of arbitrary size and type.pdf:pdf},
